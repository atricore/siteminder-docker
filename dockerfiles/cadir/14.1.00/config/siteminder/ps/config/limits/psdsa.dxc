# size limits
set max-users = 1000;
set credits = 5;
set max-local-ops = 1000;
set max-op-size = 10000;
set multi-write-queue = 20000;

# time limits
set max-bind-time = none;
set bind-idle-time = 3600;
set max-op-time = 600;

